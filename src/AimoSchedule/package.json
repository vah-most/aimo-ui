  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.22.5",
    "run-script-os": "^1.1.6"
  },
  "dependencies": {
    "moment": "^2.29.4",
    "react-moment": "^1.1.3"
