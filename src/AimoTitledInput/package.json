{
  "name": "@aimo.ui/aimo-titledinput",
  "version": "1.3.3",
  "description": "Input component with sticky title and hide/unhide option for passwords",
  "main": "index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "index.js",
    "dist/*.css",
    "dist/*.js",
    "dist/types/*.ts"
  ],
  "scripts": {
    "cleandist": "run-script-os",
    "cleandist:win32": "del /S /Q .\\dist\\*",
    "cleandist:default": "rm -R ./dist/*",
    "prepublish": "npm run cleandist & node-sass ./ -o dist/ & npx babel ./ --out-dir dist --copy-files",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vah-most/aimo-ui.git"
  },
  "keywords": [
    "input",
    "password",
    "labeled-input",
    "hide/unhide password",
    "multiline input"
  ],
  "author": "Mostafa Vahabzadeh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vah-most/aimo-ui/issues"
  },
  "homepage": "https://github.com/vah-most/aimo-ui#readme",
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "run-script-os": "^1.1.6"
  }
}
